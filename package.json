		"jspdf": "^3.0.4",
		"lucia": "^3.2.2",
		"lucide-svelte": "^0.562.0",
		"nodemailer": "^7.0.11",
		"prisma": "^5.22.0",
		"sveltekit-superforms": "^2.29.1",
		"zod": "^4.2.1"
