		"@tailwindcss/typography": "^0.5.19",
		"@types/bcryptjs": "^2.4.6",
		"@types/nodemailer": "^7.0.4",
		"autoprefixer": "^10.4.23",
		"postcss": "^8.5.6",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.18",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
